{
	"name": "@fdocs/pdf",
	"version": "1.0.0-beta.1",
	"description": "A simple light to extract plain text from a pdf file.",
	"author": "ngtuonghy",
	"license": "MIT",
	"keywords": [
		"react-pdf",
		"pdftotext",
		"pdf",
		"text",
		"react",
		"pdf2text",
		"pdfjs",
		"pdf-to-text",
		"@fdocs"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ngtuonghy/fdocs"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "tsc --noEmit && pkgroll"
	},
	"engines": {
		"node": ">=20.0.0"
	},
	"devDependencies": {
		"@types/node": "^22.5.5",
		"typescript": "^5.6.2"
	},
	"dependencies": {
		"pdfjs-dist": "^4.6.82"
	}
}
